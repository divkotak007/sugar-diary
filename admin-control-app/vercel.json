{
    "buildCommand": "cd admin-control-app && npm install && npm run build",
    "outputDirectory": "admin-control-app/dist",
    "installCommand": "cd admin-control-app && npm install",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}